    "def Date (Day, Month, Year):\n",
    "    Months = [31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31]\n",
    "    if (Year%4==0 and Year%100!=0) or Year%400==0:\n",
    "        Months[1] = 29\n",
    "    return (sum(Months[:Month-1])+Day)\n",
    "\n",
    "Date(5,10,2400)"
   ]
  }
