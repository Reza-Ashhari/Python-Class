    "def Fibo(arg):\n",
    "    if arg == 1:\n",
    "        return 0\n",
    "    elif arg == 2:\n",
    "        return 1\n",
    "    else:\n",
    "        return Fibo(arg-1) + Fibo(arg-2)\n",
    "\n",
    "Fibo(10)\n"
   ]
